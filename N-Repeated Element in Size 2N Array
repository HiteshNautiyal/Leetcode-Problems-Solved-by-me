int repeatedNTimes(vector<int>& nums) {
        int freq[10000] = {0}; //we create array with (2*n) length
        int ans;
        for(int i=0;i<nums.size();i++)
        {
            freq[nums[i] - 0 ]++; //map/count kro
            if(freq[nums[i] - 0] > 1)//jo bh 1 se jyda baar aarha hai direct loop break kro print krdo...
            {
                ans = nums[i];
            }
            

        } 
        
        return ans;
        
    }
